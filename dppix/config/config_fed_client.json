{
  "format_version": 2,

  "executors": [
    {
      "tasks": ["train", "submit_model", "get_weights"],
      "executor": {
        "path": "pix_trainer.PixTrainer",
        "args": {
          "lr": 0.0002,
          "epochs": 10
        }
      }
    }
  ],
  "task_result_filters": [
    {
      "tasks": ["train"],
      "filters":[
          {
          "path": "nvflare.app_common.filters.svt_privacy.SVTPrivacy",
          "args": {
            "fraction": 0.9,
            "epsilon": 0.001,
            "noise_var": 1.0,
            "gamma": 1e-4
          }
        }
      ]
    }
  ],
  "task_data_filters": [
  ],
  "components": [
  ]
}
